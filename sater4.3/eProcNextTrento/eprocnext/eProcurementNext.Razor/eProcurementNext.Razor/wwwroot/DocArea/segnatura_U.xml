<?xml version="1.0"?>
<Segnatura>
	<Intestazione>

		<Oggetto>@@@OGGETTO@@@</Oggetto>

		<Identificatore>
			<CodiceAmministrazione>@@@CODICEAMMINISTRAZIONE@@@</CodiceAmministrazione>
			<CodiceAOO>@@@CODAOO@@@</CodiceAOO>
			<NumeroRegistrazione>0</NumeroRegistrazione>
			<DataRegistrazione>0</DataRegistrazione>
			<Flusso>U</Flusso>
		</Identificatore>

		<Mittente>

      <Amministrazione>
        <Denominazione>@@@DENOMINAZIONEENTE@@@</Denominazione>
        <CodiceAmministrazione>@@@CODICEAMMINISTRAZIONE@@@</CodiceAmministrazione>
        <IndirizzoTelematico tipo="smtp"></IndirizzoTelematico>
        <UnitaOrganizzativa id="@@@UNITA_ORGANIZZATIVA@@@"/>
      </Amministrazione>
      <AOO>
        <CodiceAOO>@@@CODAOO@@@</CodiceAOO>
      </AOO>     

    </Mittente>

		<Destinatario>

      @@@XML-AZIENDA@@@

		</Destinatario>

		<!-- Informazioni sul Titolario. Opzionale. -->
    <Classifica>
      <CodiceAmministrazione>@@@CODICEAMMINISTRAZIONE@@@</CodiceAmministrazione>
      <CodiceAOO>@@@CODAOO@@@</CodiceAOO>
      <CodiceTitolario>@@@CODICETITOLARIO@@@</CodiceTitolario>
    </Classifica>
    
  </Intestazione>

	<!-- Elenco documenti da protocollare. Obbligatorio -->
	<Descrizione>
		@@@ALLEGATI-PROTOCOLLO@@@
	</Descrizione>
	
  <ApplicativoProtocollo nome="Akropolis">
    <Parametro nome="TipoSpedizione" valore="PEC"/>
  </ApplicativoProtocollo>

</Segnatura>