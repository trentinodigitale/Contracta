<Segnatura xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Intestazione>

		<Oggetto>@@@Oggetto@@@</Oggetto>

		<Flusso>
			<TipoRichiesta>U</TipoRichiesta>
			<Firma>NF</Firma>
			<ForzaRegistrazione>1</ForzaRegistrazione>
		</Flusso>

		<Mittenti>
			<Mittente>
				<Amministrazione>
					<Denominazione>@@@Denominazione-Amministrazione@@@</Denominazione>
					<CodiceAmministrazione>@@@Codice-Amministrazione@@@</CodiceAmministrazione>

					<UnitaOrganizzativa tipo="temporanea">
						<Denominazione>@@@denominazione-UO@@@</Denominazione>
						<Identificativo>@@@codice-UO@@@</Identificativo>
					</UnitaOrganizzativa>
				</Amministrazione>
				<AOO>
					<Denominazione>@@@denominazione-AOO@@@</Denominazione>
					<CodiceAOO>@@@codice-AOO@@@</CodiceAOO>
				</AOO>			
			</Mittente>
		</Mittenti>    

		<Destinatari>
      @@@xml-azienda@@@
		</Destinatari>

		<!-- obbligatorio per protocollo in uscita -->
		<FascicoloPrimario>
			<CodiceAmministrazione>@@@Codice-Amministrazione@@@</CodiceAmministrazione>
			<CodiceAOO>@@@codice-AOO@@@</CodiceAOO>
			<!-- voce di titolario -->
			<Classifica>@@@titolario-fascicolo@@@</Classifica>
			<Anno>@@@anno-fascicolo@@@</Anno>
			<Progressivo>@@@progressivo-fascicolo@@@</Progressivo>
		</FascicoloPrimario>

	</Intestazione>
</Segnatura>

