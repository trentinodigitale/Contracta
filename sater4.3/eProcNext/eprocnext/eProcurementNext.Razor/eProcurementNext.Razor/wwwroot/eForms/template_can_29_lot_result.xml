<efac:LotResult>
	<!-- OPT-322 - LotResult Technical Identifier - RES-NNNN -->
	<cbc:ID>@@@LOTTO_RESULT_ID@@@</cbc:ID>
	<!-- BT-142 - Winner Chosen - Stato di selezione dei vincitori - Stato Lotto -->
	<cbc:TenderResultCode listName="winner-selection-status">@@@LOTTO_RESULT_TENDER_RESULT_CODE@@@</cbc:TenderResultCode>

	@@@LOTTO_RESULT_NO_ENCODE_DECISION_REASON@@@

	@@@LOTTO_RESULT_NO_ENCODE_LOT_TENDER@@@
	@@@LOTTO_RESULT_NO_ENCODE_MAXIMUMVALUEAMOUNT@@@

	@@@LOTTO_RESULT_NO_ENCODE_SETTLED_CONTRACT@@@

	<efac:TenderLot>
		<!-- BT-13713 - Result Lot Identifier - L'identificatore del lotto di risultato -->
		<cbc:ID>@@@LOTTO_RESULT_TENDER_LOT_ID@@@</cbc:ID>
	</efac:TenderLot>
	
</efac:LotResult>