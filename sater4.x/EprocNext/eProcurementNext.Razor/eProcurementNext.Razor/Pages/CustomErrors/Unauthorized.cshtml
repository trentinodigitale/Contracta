@page "/Unauthorized.asp"
@model eProcurementNext.Razor.Pages.Unauthorized.UnauthorizedModel
@using System.Web;
@using static eProcurementNext.Session.SessionMiddleware;
@{
	Response.StatusCode = 403;
	string motivo = string.Empty;
	if (string.IsNullOrEmpty(Request.Query["message"].ToString()))
	{
		motivo = "Unauthorized";
	}
	else
	{
		motivo = HttpUtility.UrlDecode(Request.Query["message"].ToString());
	}
}
<script type="text/javascript">
	alert('@(Html.Raw(HttpUtility.JavaScriptStringEncode(motivo)))');
	window.location.href = '@(Html.Raw(HttpUtility.JavaScriptStringEncode(DefaultHomePage)))';
</script>
