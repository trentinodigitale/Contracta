    string getBaseDomain()
    {
        string? server;
        string[] a;
        string x;

        server = Request.HttpContext.GetServerVariable("server_name");

        a = Strings.Split(server, ".");

        x = a[a.Length - 1];

        if (a.Length - 1 > 1)
        {
            x = a[a.Length - 1 - 1] + "." + a[a.Length - 1];
        }
        else
        {
            x = a[a.Length - 1];
        }

        return x;
    }

